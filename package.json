{
	"name": "vscode-md-footer",
	"displayName": "vscode-md-footer",
	"description": "Generate markdown footer links.",
	"publisher": "k-kuroguro",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Formatters"
	],
	"license": "MIT",
	"homepage": "https://github.com/k-kuroguro/vscode-md-footer/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/k-kuroguro/vscode-md-footer.git"
	},
	"bugs": {
		"url": "https://github.com/k-kuroguro/vscode-md-footer/issues"
	},
	"activationEvents": [
		"onCommand:vscode-md-footer.generateFooter"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-md-footer.generateFooter",
				"category": "VSCode Md Footer",
				"title": "Generate Footer",
				"enablement": "editorLangId == markdown"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "npm run test-copy && tsc -p ./",
		"test-watch": "npm run test-copy && tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"test-copy": "cp ./src/test/suite/test.md/ ./out/test/suite/ -r"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {}
}
