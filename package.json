{
   "name": "md-footer",
   "displayName": "Md Footer",
   "description": "Generate markdown footer links.",
   "publisher": "k-kuroguro",
   "version": "1.0.0",
   "icon": "resources/icon.png",
   "engines": {
      "vscode": "^1.57.0"
   },
   "categories": [
      "Formatters"
   ],
   "license": "MIT",
   "homepage": "https://github.com/k-kuroguro/vscode-md-footer/blob/master/README.md",
   "repository": {
      "type": "git",
      "url": "https://github.com/k-kuroguro/vscode-md-footer.git"
   },
   "bugs": {
      "url": "https://github.com/k-kuroguro/vscode-md-footer/issues"
   },
   "activationEvents": [
      "onCommand:md-footer.generateFooter"
   ],
   "main": "./dist/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "md-footer.generateFooter",
            "category": "Md Footer",
            "title": "Generate Footer",
            "enablement": "editorLangId == markdown"
         }
      ]
   },
   "scripts": {
      "vscode:prepublish": "npm run package",
      "webpack": "webpack --mode development",
      "watch": "webpack --watch",
      "package": "webpack --mode production --devtool hidden-source-map",
      "test-compile": "npm run webpack && npm run test-copy && tsc -p ./",
      "test-watch": "npm run webpack && npm run test-copy && tsc -watch -p ./ ",
      "pretest": "npm run test-compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js",
      "test-copy": "npx cp-cli ./src/test/suite/test.md/ ./out/test/suite/test.md/"
   },
   "devDependencies": {
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.0.4",
      "@types/node": "^12.11.7",
      "@types/vscode": "^1.57.0",
      "@typescript-eslint/eslint-plugin": "^4.14.1",
      "@typescript-eslint/parser": "^4.14.1",
      "cp-cli": "^2.0.0",
      "eslint": "^7.19.0",
      "mocha": "^8.2.1",
      "ts-loader": "^8.0.14",
      "typescript": "^4.1.3",
      "vscode-test": "^1.5.0",
      "webpack": "^5.19.0",
      "webpack-cli": "^4.4.0"
   },
   "dependencies": {
      "node": "^16.4.0"
   }
}
